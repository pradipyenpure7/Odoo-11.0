	<record id="approval_types_info_form" model="ir.ui.view">
		<field name="name">	Approval Types</field>
		<field name="model">approval_types.info</field>
		<field name="arch" type="xml">
			<form string="Role Types">
				<sheet>
					<group>
						<field name="approval_type" string="Role"/>
					</group>
					<notebook>
						<page string="Users">
						<field name="role_line" mode="tree">
							<tree string="Users" editable="bottom">
								<field name="approval_id" invisible="1"/>
								<field name="users" string="User"/>		
							</tree>
						</field>
						</page>
					</notebook>
				</sheet>
			</form>
		</field>
	</record>

	<record id="approval_types_info_tree" model="ir.ui.view">
