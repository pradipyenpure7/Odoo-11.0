<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data>                    
    
      <record id="sunfire_support_ticket_channel_view_tree" model="ir.ui.view">
          <field name="name">sunfire.support.ticket.channel.tree.view</field>
          <field name="model">sunfire.ticket.channel</field>
          <field name="arch" type="xml">
      	      <tree editable="top">
      	          <field name="name"/>
      	      </tree>
      	  </field>
      </record>

      <record id="sunfire_support_ticket_channel_action" model="ir.actions.act_window">
          <field name="name">Support Ticket Channels</field>
	  <field name="res_model">sunfire.ticket.channel</field>
	  <field name="view_mode">tree</field>
	  <field name="help" type="html">
	      <p>
	          No Support Tickets Tags found
	      </p>
          </field>
      </record>
 
    </data>
</odoo>